{
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/ai/:path*",
      "destination": "/ai/index.html"
    },
    {
      "source": "/records/:path*",
      "destination": "/records/index.html"
    },
    {
      "source": "/tools/:path*",
      "destination": "/tools/index.html"
    },
    {
      "source": "/settings/:path*",
      "destination": "/settings/index.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/privacy",
      "destination": "/privacy.html"
    },
    {
      "source": "/manifesto",
      "destination": "/manifesto.html"
    }
  ]
}

